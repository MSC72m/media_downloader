      "^https?://music\\.youtube\\.com/watch\\?v=[\\w-]+",
      "^https?://music\\.youtube\\.com/playlist\\?list=[\\w-]+"
    ],
    "youtube_domains": ["www.youtube.com", "youtube.com", "youtu.be"],
